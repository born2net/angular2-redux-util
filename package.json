{
    "name": "angular2-redux-util",
    "version": "0.5.38",
    "description": "Wrapper components for using Redux in an Angular2 application",
    "main": "dist/index.js",
    "typings": "dist/index.d.ts",
    "scripts": {
        "lint": "tslint src/**/*.ts -s node_modules/tslint-stylish -t stylish",
        "commit": "git-cz",
        "build & copy": "npm run build && cp -r -f ./dist/ ../studioEnterprise/node_modules/angular2-redux-util/",
        "clean": "rimraf dist ",
        "build": "tsc --outDir dist --declaration",
        "test": "karma start",
        "build-and-test": "npm run clean && npm run lint && npm run test && npm run build",
        "semantic-release": "semantic-release pre && npm publish && semantic-release post",
        "complete": "gulp bump && npm run build && npm publish ./"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/born2net/angular2-redux-util.git"
    },
    "keywords": [
        "angular",
        "angular2",
        "angular 2",
        "redux"
    ],
    "author": "Ruby Boyarski <rubyboy@gmail.com>",
    "license": "MIT",
    "bugs": {
        "url": "https://github.com/InfomediaLtd/angular2-redux/issues"
    },
    "homepage": "https://github.com/born2net/angular2-redux-util#readme",
    "peerDependencies": {
        "redux": "^3.x"
    },
    "devDependencies": {
        "commitizen": "^2.4.6",
        "cz-conventional-changelog": "^1.1.5",
        "ghooks": "^1.0.1",
        "gulp-bump": "^1.0.0",
        "gulp-tslint": "^4.2.2",
        "jasmine-core": "^2.4.1",
        "karma": "^0.13.15",
        "karma-jasmine": "^0.3.6",
        "karma-phantomjs-launcher": "^0.2.1",
        "phantomjs": "^1.9.19",
        "redux": "^3.x",
        "rimraf": "^2.4.4",
        "semantic-release": "^4.3.5",
        "tsc": "^1.20150623.0",
        "tslint": "^3.2.1",
        "tslint-stylish": "^2.1.0-beta",
        "typescript": "^1.7.5"
    },
    "config": {
        "commitizen": {
            "path": "node_modules/cz-conventional-changelog"
        },
        "ghooks": {
            "pre-commit": "npm run build-and-test"
        }
    },
    "directories": {
        "test": "test"
    },
    "dependencies": {
        "is-equal": "^1.4.2",
        "object-path": "^0.9.2"
    }
}
